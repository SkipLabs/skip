{
  "name": "skdb",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/skdb.js",
  "exports": {
    ".": "./dist/skdb.js"
  },
  "scripts": {
    "build": "tsc && skargo build -r --target wasm32-unknown-unknown --lib --manifest-path=../Skargo.toml --out-dir=./dist/",
    "clean": "rm -rf dist",
    "lint": "eslint",
    "cli": "node ./dist/skdb-cli.js"
  },
  "bin": {
    "skdb-cli": "./dist/skdb-cli.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.2",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.12"
  },
  "dependencies": {
    "@skip-wasm/std": "^1.0.0",
    "@skip-wasm/date": "^1.0.0",
    "@skip-wasm/json": "^1.0.0"
  }
}
