@export("SkipRuntime_Runtime__subscribe")
fun subscribeOfRuntime(
  reactiveId: String,
  from: Int,
  notifier: Notifier,
  reactiveAuth: ?Array<UInt8>,
): Int {
  SKStore.runWithResult(context ~> {
    subscribe(
      context,
      reactiveId,
      SKStore.Tick(from),
      notifier.notify,
      reactiveAuth,
    )
  }) match {
  | Success(id) -> id
  | Failure(err) -> -getErrorHdl(err).toInt()
  };
}

