class Notifier(eptr: SKStore.ExternalPointer) {
  fun notify(
    values: Array<(SKJSON.CJSON, Array<SKJSON.CJSON>)>,
    tick: SKStore.Tick,
    updates: Bool,
  ): void {
    notifyOfNotifier(
      this.eptr.value,
      SKJSON.CJArray(
        values.map(v -> SKJSON.CJArray(Array[v.i0, SKJSON.CJArray(v.i1)])),
      ),
      tick.value,
      Int32::truncate(if (updates) 1 else 0),
    )
  }
}

