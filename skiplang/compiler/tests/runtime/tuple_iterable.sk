fun main(): void {
  // Tuple2
  for (x in (1, 2)) print_raw(x.toString());
  print_raw("\n");
  // Tuple3
  for (x in (1, 2, 3)) print_raw(x.toString());
  print_raw("\n");
  // Tuple4
  for (x in (1, 2, 3, 4)) print_raw(x.toString());
  print_raw("\n");
  // String tuples
  for (s in ("a", "b", "c")) print_raw(s);
  print_raw("\n");
}
