class StrExn(x: String) extends Exception {
  fun getMessage(): String {
    this.x
  }
}

fun foo(): String {
  throw StrExn("Pass\n")
}

fun main(): void {
  print_raw(
    vtry(
      () -> {
        foo() + throw StrExn("Fail")
      },
      exn ->
        exn match {
        | StrExn(s) -> s
        | _ -> "Fail"
        }
      ,
    ),
  )
}
