fun main(): void {
  print_raw(
    vtry(
      () -> {
        foo()
      },
      exn ->
        exn match {
        | _ -> "Pass\n"
        }
      ,
    ),
  )
}

class MyException() extends Exception {
  fun getMessage(): String {
    "MyException error"
  }
}

fun foo<T>(): T {
  throw MyException()
}
