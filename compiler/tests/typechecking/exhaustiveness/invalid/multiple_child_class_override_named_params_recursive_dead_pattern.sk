base class A{x: Int}
base class B{y: Bool} extends A
class C{z: A} extends B

fun myMatch(x: A): String {
  x match {
  | A{x => 1} -> "P"
  | B{y => false} -> "a"
  | C{z => A{x => 1}, y => true, x => 2} -> "s"
  | C{z => B{y => true, x => 1}, y => true, x => 2} -> "s"
  | B{y => true, x => 2} -> "s"
  | A{x => _} -> "\n"
  }
}
