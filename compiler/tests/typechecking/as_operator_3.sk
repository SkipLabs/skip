base class P
base class Q extends P
base class R extends P
class S() extends Q, R
class T() extends Q, R

fun main(): void {
  x: P = S();
  vtry(
    () -> {
      _y = x as T _;
      print_string("Fail")
    },
    exn ->
      exn match {
      | InvalidCast _ -> print_string("Pass")
      | _ -> void
      }
    ,
  )
}
