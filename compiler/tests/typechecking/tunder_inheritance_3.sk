base class Foo {
  overridable fun hello(): String {
    "String"
  }
}

class Bar() extends Foo {
  fun hello(): _ {
    invariant_violation("Expected invariant")
  }
}

fun main(): void {
  print_raw(
    vtry(
      () -> {
        Bar().hello()
      },
      exn ->
        exn match {
        | _ -> "Pass\n"
        }
      ,
    ),
  )
}
