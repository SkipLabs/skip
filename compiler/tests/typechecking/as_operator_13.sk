class Foo(int: Int) {}

const FOO: Foo = Foo(0);

fun main(): void {
  x = Foo(0);
  _x = x as FOO;
  vtry(
    () -> {
      y = Foo(1);
      _y = y as FOO;
      print_string("Fail")
    },
    exn ->
      exn match {
      | InvalidCast _ -> print_string("Pass")
      | _ -> print_string("Fail")
      }
    ,
  )
}
