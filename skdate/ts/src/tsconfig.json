{
  "compilerOptions": {
    "target": "ES2021",
    "outDir": "../dist",
    "baseUrl": ".",
    "paths": {
      "#std/*": ["../../../prelude/ts/src/*"],
      "#skdate/*": ["./*"]
    },
    "strictNullChecks": true,
    "module": "node16",
    "strict": true,
    "verbatimModuleSyntax": true,
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "moduleResolution": "Node16",
    "incremental": true,
    "noEmit": true
  }
}
